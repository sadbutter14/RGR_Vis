<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="DB_Classes.DataEntity" table="Data" schema="RGR">
        <id name="id">
            <column name="id" sql-type="int(2)"/>
        </id>
        <property name="age">
            <column name="age" sql-type="char(255)"/>
        </property>
        <property name="category">
            <column name="category" sql-type="tinytext"/>
        </property>
        <property name="name">
            <column name="name" sql-type="char(1)" length="1" not-null="true"/>
        </property>
        <property name="description">
            <column name="description" sql-type="text"/>
        </property>
    </class>
</hibernate-mapping>